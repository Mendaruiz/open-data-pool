<?xml version="1.0" encoding="UTF-8"?>
<Mapping xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:ei2a="http://opendata.aragon.es/def/ei2a#" xmlns:org="http://www.w3.org/ns/org#" xmlns:vcard="http://www.w3.org/2006/vcard/ns#" id="19">
  <Entity rdf:type="org:Organization" URI="http://opendata.aragon.es/pool/entidad-mayor/{ELM_ID}">
    <Property field="{FUENTE}" attribute="dc:source" type="string"/>
    <Property field="{ELM_ID}" attribute="dc:identifier" type="string"/>
    <Property field="{DENOMINACION}" attribute="ei2a:organizationName" type="string"/>
    <Property field="{TELEFONO}" attribute="ei2a:phone" type="string"/>
    <Property field="{EMAIL}" attribute="foaf:mbox" type="string"/>
    <Property field="{CIF}" attribute="ei2a:CIF" type="string"/>
    <Property field="{FC_PUBLICACION_BOA}" attribute="ei2a:officialCreationDate" type="dateTime"/>
    <Property link="ei2a:Ayuntamiento" attribute="ei2a:publicEntityType"/>
    <Property link="ei2a:órganoAdministrativo" attribute="ei2a:organizationType"/>
    <Property link="ei2a:AdministraciónLocal" attribute="ei2a:administrativeLevel"/>
    <Property fieldLink="http://opendata.aragon.es/pool/municipio/{CODIGO_MUN}" attribute="ei2a:location"/>
    <Property fieldLink="http://opendata.aragon.es/pool/sitesMayor/{ELM_ID}" attribute="org:hasSite">
      <Entity rdf:type="org:Site" URI="http://opendata.aragon.es/pool/sitesMayor/{ELM_ID}">
        <Property field="{FUENTE}" attribute="dc:source" type="string"/>
        <Property fieldLink="http://opendata.aragon.es/pool/vCardMayor/{ELM_ID}" attribute="org:siteAddress">
          <Entity rdf:type="vcard:VCard" URI="http://opendata.aragon.es/pool/vCardMayor/{ELM_ID}">
            <Property field="{FUENTE}" attribute="dc:source" type="string"/>
            <Property field="{FAX}" attribute="vcard:Fax" type="string"/>
            <Property fieldLink="http://opendata.aragon.es/pool/adrMayor/{ELM_ID}" attribute="vcard:hasAddress">
              <Entity rdf:type="vcard:ADR" URI="http://opendata.aragon.es/pool/adrMayor/{ELM_ID}">
                <Property field="{FUENTE}" attribute="dc:source" type="string"/>
                <Property field="{DIRECCION}" attribute="vcard:street-address" type="string"/>
                <Property field="{MUNICIPIO}" attribute="vcard:locality" type="string"/>
                <Property field="{CP}" attribute="vcard:postal-code" type="string"/>
              </Entity>
            </Property>
          </Entity>
        </Property>
      </Entity>
    </Property>
  </Entity>
</Mapping>
